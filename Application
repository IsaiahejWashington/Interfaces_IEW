
public class Application {

	public static void main(String[] args) {
		Task task1 = new Task("Task 1");
		Task task2 = new Task("Task 2");
		Process process1 = new Process("Process 1");
		Process process2 = new Process("Process 2");
		
		task1.setPriority(Priority.MIN_PRIORITY);
		task2.setPriority(Priority.MAX_PRIORITY);
		
		process1.setPriority(Priority.MED_PRIORITY);
		process2.setPriority(Priority.MAX_PRIORITY);
		
		
		
		
		System.out.println(task1);
		System.out.println(task2);
		System.out.println(process1);
		System.out.println(process2);
		
		System.out.println("Task comparison: "+ task1.compareTo(task2));
		System.out.println("Process comparison: "+ process1.compareTo(process2));
		
		
	
		
	}//end class

	

}//end main
